<?xml version="1.0"?>
<!DOCTYPE benchmark PUBLIC "+//IDN sosy-lab.org//DTD BenchExec benchmark 1.18//EN" "https://www.sosy-lab.org/benchexec/benchmark-1.18.dtd">
<benchmark tool="dartagnan" timelimit="1 min" hardtimelimit="1 min" memlimit="5 GB" cpuCores="1">

<resultfiles>**.graphml</resultfiles>
      
<rundefinition name="SV-COMP-benchmarks-thesis">
  <tasks name="X86">
    <option>cat/power.cat</option>
    <option>power</option>
    <propertyfile>unreach-call.prp</propertyfile>
    <includesfile>thesis/out/litmus/ppc.set</includesfile>
  </tasks> 

  </rundefinition>
</benchmark>
